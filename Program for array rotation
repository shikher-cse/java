public static void main(String[] args){  
		int arr[] = new int[] {1,2,3,4,5};
		//expected  output is : 4,5,3,1,2 as  because of  array  rotation
		
		int temp[] =new int[2];
		temp[0] = arr[0];
		temp[1] = arr[1];
		int d =arr[arr.length-2],n = arr[arr.length-1];
	    arr[0] =d;
	    arr[1] =n;
	    arr[arr.length-2]= temp[0];
	    arr[arr.length-1] = temp[1];
	    for(int i:arr) {
	    	System.out.println(i);
	    }
		
		
		
	}
